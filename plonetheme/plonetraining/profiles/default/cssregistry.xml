<?xml version="1.0"?>
<object name="portal_css">

 <stylesheet
    id="++theme++plonetheme.plonetraining/css/theme.css"
    expression="request/HTTP_X_THEME_ENABLED | nothing"
    applyPrefix="1"
    media=""/>

 <!-- columns.css is deco grid but conflict with bootstrap grid -->
 <stylesheet id="columns.css" enabled="0"/>
 <!-- mobile.css conflict because it needs sunburst's viewport -->
 <stylesheet id="mobile.css" enabled="0"/>


 <stylesheet
    id="++resource++collective.js.bootstrap/css/bootstrap.min.css"
    applyPrefix="True"
    media="" rel="stylesheet" rendering="link"
    cacheable="True" compression="safe" cookable="True"
    enabled="1" expression=""/>

 <stylesheet title=""
    id="++resource++collective.js.bootstrap/css/bootstrap-responsive.min.css"
    applyPrefix="True"
    media="" rel="stylesheet" rendering="link"
    cacheable="True" compression="safe" cookable="True"
    enabled="1" expression=""/>

 <!-- Deactivate emtpy CSS stylesheet that come with Sunburst theme -->
 <stylesheet title=""
    id="base.css"
    media="screen" rel="stylesheet" rendering="link"
    cacheable="True" compression="safe" cookable="True"
    enabled="0" expression=""/>

</object>
